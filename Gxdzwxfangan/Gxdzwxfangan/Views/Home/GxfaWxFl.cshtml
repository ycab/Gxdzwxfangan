        var navid = "@ViewBag.navid";
        //alert(navid);


        var page_Init = 0;//页面初始化变量,SortNameInfo与SortGoodsInfo函数异步请求结束后再显示页面
        //初始化，异步加载大分类名字

        ///初始化，异步加载中分类的名字信息
        function MiddleSortNameInfo() {
            $.ajax({
                type: "POST",
                url: rootUrl + "Home/MiddleSortNameInfo?random=" + Math.floor(Math.random() * (100000 + 1)),
                dataType: "json",
                data: { middlesortname_last: MiddleSortName_Last, middlesortname_amount: MiddleSortName_Amount },
                async: true,
                success: function (obj) {
                    if (obj.msg == "success") {
                        var middleinfo = obj.middlesortinfo;//middleinfo是整个信息
                        $("#showgoods").html("");
                        for (var i = 0; i < middleinfo.length; i++) {
                            var middle = middleinfo[i];//{parentid:1,parentname:'',info:[{:'',...},{},{}]}
                            //var parentid = middle.parentid;middle是一个职业的所有信息
                            var task_category_name = middle.task_category_name;
                            var task_category_id = middle.task_category_id;
                            var info = middle.info;//[{:'',.....},{},{}]某个职业单条数据的集合.例如：info[0].USER_NAME，某职业第一条数据的人名
                            if (i == 0) {
                                $("#showgoods").append(
                                "<section class='menu-right padding-all j-content' data-parentid='" + task_category_id + "'>"
                               + "<h5>" + task_category_name + "</h5>"
                               + "</section>");

                            }
                            else {
                                $("#showgoods").append(
                                "<section class='menu-right padding-all j-content' style='display:none' data-parentid='" + task_category_id + "'>"
                                + "<h5>" + task_category_name + "</h5>"
                                + "</section>");
                            }
                            var domli = "<ul class='fb-list'>";
                            for (var j = 0; j < info.length; j++) {
                                if (info[j].IS_RECEIVED == "1")
                                {
                                    info[j].IS_RECEIVED = "竞标中";
                                }
                                else if (info[j].IS_RECEIVED == "2")
                                {
                                    info[j].IS_RECEIVED = "竞标成功";
                                }
                                else if (info[j].IS_RECEIVED == "3")
                                {
                                    info[j].IS_RECEIVED = "托管中";
                                }
                                else if (info[j].IS_RECEIVED == "4")
                                {
                                    info[j].IS_RECEIVED = "已完成";
                                }
                                domli += "<ul class='fb-list ulborder'>"
                                     + '<a onclick="javascript:task_list(\'' + info[j].TASK_ID + '\');">'
                                       + "<li>"
                                         + "<div class='middle' style='width:60%;'>"
                                           + "<h2>"+info[j].TASK_NAME+"</h2>"
                                           + "<p>"+info[j].SEND_TIME+"</p>"
                                           + "</div>"
                                           + "<div class='right' style='width:40%;'>"
                                             + "<h2>￥" + info[j].PRICE + "</h2>"
                                             + "<p>"+info[j].APPLY_NUMBER+"人申请"
                                             + "<span>"+info[j].IS_RECEIVED+"</span>"
                                             + "</p>"
                                           + "</div>"
                                          + "</li>"
                                          +"</a>"
                                     +"</ul>"

                            }
                            domli+="</ul>"
                            $("section[data-parentid='" + task_category_id + "']").append(domli);


                                   }
                    }
                    else {
                        $('#loading').hide();
                    }

                },
                error: function () {
                    alert("ajax wrong!");
                    $('#loading').hide();
                }
            });
        }
